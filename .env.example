# Provider-specific model names
LLM_MODEL=models/gemini-2.5-flash

# Provider API Keys (get from respective provider websites)
# Google Gemini API Key - Get from: https://makersuite.google.com/app/apikey
GOOGLE_API_KEY=your_google_api_key_here

# Performance Tuning
LLM_TIMEOUT=30
LLM_TEMPERATURE=0.75

# VTube Studio Configuration
VTS_URL=ws://localhost:8001

# Audio Configuration
# Set the audio output device index (run audio_player/listAudioDevices.py to see available devices)
# Leave commented to use system default
# OUTPUT_DEVICE_INDEX=6

# TTS (Text-to-Speech) Configuration
TTS_ENABLED=true